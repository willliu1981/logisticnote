<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.ProductExchangeCreateActivity"
    android:focusable="true"
    android:focusableInTouchMode="true"
    >

    <!-- pe -->


    <EditText
        android:id="@+id/pe.edtv_shelf_from_for_update"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="1A00A11"
        android:inputType="textVisiblePassword"
        android:textAllCaps="true"
        app:layout_constraintStart_toStartOf="@+id/textView28"
        app:layout_constraintTop_toBottomOf="@+id/textView28" />

    <EditText
        android:id="@+id/pe.edtv_shelf_to_for_update"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:hint="1A00B11"
        android:inputType="textPersonName"
        android:textAllCaps="true"
        app:layout_constraintStart_toEndOf="@+id/textView30"
        app:layout_constraintTop_toTopOf="@+id/pe.edtv_shelf_from_for_update"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="新儲位"
        app:layout_constraintStart_toEndOf="@+id/textView30"
        app:layout_constraintTop_toTopOf="@+id/textView28"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="▷"
        app:layout_constraintBottom_toBottomOf="@+id/pe.edtv_shelf_from_for_update"
        app:layout_constraintStart_toEndOf="@+id/pe.edtv_shelf_from_for_update"
        app:layout_constraintTop_toTopOf="@+id/pe.edtv_shelf_from_for_update"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="原儲位"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <Button
        android:id="@+id/pe.btn_exchange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="交換"
        app:layout_constraintBottom_toBottomOf="@+id/pe.edtv_shelf_to_for_setup"
        app:layout_constraintStart_toEndOf="@+id/pe.edtv_shelf_to_for_setup"
        app:layout_constraintTop_toTopOf="@+id/pe.edtv_shelf_to_for_setup"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginLeft="8dp" />

    <Button
        android:id="@+id/pe.btn_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="修改記錄"
        app:layout_constraintBottom_toBottomOf="@+id/pe.edtv_shelf_to_for_update"
        app:layout_constraintStart_toEndOf="@+id/pe.edtv_shelf_to_for_update"
        app:layout_constraintTop_toTopOf="@+id/pe.edtv_shelf_to_for_update"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="▷"
        app:layout_constraintBottom_toBottomOf="@+id/pe.edtv_shelf_from_for_setup"
        app:layout_constraintStart_toEndOf="@+id/pe.edtv_shelf_from_for_setup"
        app:layout_constraintTop_toTopOf="@+id/pe.edtv_shelf_from_for_setup"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="產品編號"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pe.txtv_title"
        android:layout_marginLeft="24dp" />

    <EditText
        android:id="@+id/pe.edtv_pno"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="P40001680"
        android:inputType="textVisiblePassword"
        android:textAllCaps="true"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/pe.edtv_shelf_from_for_setup"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="1A00B11"
        android:inputType="textPersonName"
        android:textAllCaps="true"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/pe.edtv_shelf_to_for_setup"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="1A00C11"
        android:inputType="textPersonName"
        android:textAllCaps="true"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="原儲位"
        app:layout_constraintStart_toStartOf="@+id/textView28"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="新儲位"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/pe.txtv_setupDate"
        android:layout_width="165dp"
        android:layout_height="30dp"
        android:background="@drawable/textview_border"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="日期"
        app:layout_constraintStart_toStartOf="@+id/pe.edtv_shelf_from_for_update"
        app:layout_constraintTop_toBottomOf="@+id/pe.edtv_shelf_from_for_update" />

    <TextView
        android:id="@+id/pe.txtv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="庫移紀錄"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="下一筆"
        app:layout_constraintBottom_toBottomOf="@+id/button2"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toTopOf="@+id/button2"
        android:layout_marginLeft="8dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="上一筆"
        app:layout_constraintStart_toStartOf="@+id/pe.edtv_pno"
        app:layout_constraintTop_toBottomOf="@+id/pe.edtv_pno" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="前記錄"
        app:layout_constraintStart_toStartOf="@+id/button2"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="新紀錄"
        app:layout_constraintStart_toStartOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/pe.txtv_setupDate" />

</androidx.constraintlayout.widget.ConstraintLayout>